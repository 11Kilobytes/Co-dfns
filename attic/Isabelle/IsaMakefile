
## targets

default: CoDfns
images: 
test: CoDfns

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i true -d pdf  ## -D generated


## CoDfns

CoDfns: $(LOG)/HOL-CoDfns.gz

$(LOG)/HOL-CoDfns.gz: ## CoDfns/ROOT.ML CoDfns/document/root.tex CoDfns/*.thy
	@$(USEDIR) HOL CoDfns


## clean

clean:
	@rm -f $(LOG)/HOL-CoDfns.gz
